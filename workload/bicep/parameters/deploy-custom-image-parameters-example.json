{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "deploymentPrefix": {
            "value": "<<deploymentPrefix>>"
        },
        "avdSharedServicesLocation": {
            "value": "eastus2"
        },
        "avdSharedServicesSubId": {
            "value": "<<subscriptionId>>"
        },
        "avdUseAvailabilityZones": {
            "value": false
        },
        "aibLocation": {
            "value": "eastus2"
        },
        "createAibCustomRole": {
            "value": true
        },
        "avdOsImage": {
            "value": "win10_21h2"
        },
        "useSharedImage": {
            "value": true
        },
        "createAibManagedIdentity": {
            "value": true
        },
        "existingAibManagedIdentityId": {
            "value": ""
        },
        "existingAibManagedIdentityName": {
            "value": ""
        },
        "avdUseCustomNaming": {
            "value": false
        },
        "createResourceTags": {
            "value": false
        },
        "useExistingVirtualNetwork": {
            "value": false
        },
        "enableMonitoringAlerts": {
            "value": false
        }
    }
