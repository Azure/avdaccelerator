{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "deploymentPrefix": {
            "value": "AVD1"
        },
        "avdWorkloadSubsId": {
            "value": "<<subscriptionId>>"
        },
        "avdVmLocalUserName": {
            "value": "<<localUserName>>"
        },
        "avdVmLocalUserPassword": {
            "value": "<<localUserPassword>>"
        },
        "identityDomainName": {
            "value": "none"
        },
        "avdDomainJoinUserName": {
            "value": "<<domainJoinUserName>>"
        },
        "avdDomainJoinUserPassword": {
            "value": "<<domainJoinUserPassword>>"
        },
        "customDnsIps": {
            "value": [
                "<<customDnsIp>>"
            ]
        }
    }
}
